{
  "mcpServers": {
    "weather-mcp-stdio": {
      "disabled": false,
      "command": "uvx",
      "args": [
        "--from",
        ".",
        "--directory",
        "mcp-servers/weather-mcp-server",
        "mcp-server",
        "--transport",
        "stdio"
      ],
      "env": {
        "DEBUG": "1",
        "LOG_LEVEL": "INFO"
      }
    },
    "weather-mcp-http": {
      "disabled": true,
      "url": "http://weather-mcp:8080/mcp"
    }
  }
}
